drop table if exists main.sdc;
create table main.sdc (
index_name	varchar(500),
index_code	varchar(500),
index_key	varchar(500),
effective_date	varchar(500),
company	varchar(500),
ric	varchar(500),
bloomberg_ticker	varchar(500),
cusip	varchar(500),
isin	varchar(500),
sedol	varchar(500),
ticker	varchar(500),
gv_key	varchar(500),
stock_key	varchar(500),
gics_code	varchar(500),
dji_industry_code	varchar(500),
alternate_classification_code	varchar(500),
mic	varchar(500),
country_of_domicile	varchar(500),
country_of_listing	varchar(500),
region	varchar(500),
size	varchar(500),
cap_range	varchar(500),
currency_code	varchar(500),
local_price	varchar(500),
fx_rate	varchar(500),
shares_outstanding	varchar(500),
market_cap	varchar(500),
iwf	varchar(500),
awf	varchar(500),
growth	varchar(500),
value	varchar(500),
index_shares	varchar(500),
index_market_cap	varchar(500),
index_weight	varchar(500),
daily_price_return	varchar(500),
daily_total_return	varchar(500),
dividend	varchar(500),
net_dividend	varchar(500),
info_type	varchar(500),
record_date	date,
UNIQUE(index_name, ric, info_type, record_date)
);

drop table if exists main.sdl;
create table main.sdl(
change	varchar(500),
date_of_index	varchar(500),
index_name	varchar(500),
index_code	varchar(500),
index_key	varchar(500),
gics_code	varchar(500),
iso_code	varchar(500),
index_value	varchar(500),
close_market_cap	varchar(500),
close_divisor	varchar(500),
close_count	varchar(500),
daily_return	varchar(500),
index_dividend	varchar(500),
adj_market_cap	varchar(500),
adj_divisor	varchar(500),
adj_count	varchar(500),
record_date	date,
UNIQUE(index_name, date_of_index, record_date)
);

drop table if exists main.sde;
create table main.sde (
index_name	varchar(500),
index_code	varchar(500),
